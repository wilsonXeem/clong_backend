{
  "name": "clong-backend",
  "version": "1.0.0",
  "description": "Backend API for CloNG website with Express, TypeScript, Drizzle ORM, PostgreSQL, and Cloudinary",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "vercel-build": "npm run build",
    "migrate": "drizzle-kit generate && drizzle-kit push",
